%strong Add people:
%br/

= form_with(model: @transactions, url: transaction_update_users_path(@transactions), local: true) do |form|
  = form_for (:users) do |user_form|
    %p
      = user_form.label :name
      = user_form.text_field :name
  
      &nbsp;&nbsp;
    
      paid &nbsp;&nbsp;
      = user_form.text_field :paid

      &nbsp;&nbsp;
      proportion:
      = user_form.text_field :proportion
      %br/
  
    %p
      = user_form.submit

